<Window x:Class="Wpf_Learning_First.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Wpf_Learning_First"
        mc:Ignorable="d"
        Title="Hello Wpf" Height="350" Width="525">
  <DockPanel >
      <Button Height="25" Background="Red" DockPanel.Dock="Top"></Button>
      <Button Width="25" Background="Green" DockPanel.Dock="Left"></Button>
      <Button Background="Black" Height="25" DockPanel.Dock="Bottom"></Button>
      <Button Height="25" Background="Yellow" DockPanel.Dock="Right"></Button>
       <Button Background="Turquoise"></Button>
  </DockPanel>
           

</Window>
